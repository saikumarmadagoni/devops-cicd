pipeline {

agent any 

stages {



stage("ansible"){
agent any 

steps {
       checkout scm
       script {
         sh "ls -ltrh"
          sh "whoami"
          sh "sudo ls"
        }
       ansiblePlaybook disableHostKeyChecking: true,installation: 'Ansible',inventory: 'ansible/sample/hosts.ini', playbook: 'ansible/sample/playbook.yaml'

}

}
}
}
